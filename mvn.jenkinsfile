pipeline {
    agent { 
        node { label "master" } 
    }

    parameters {
        choice(choices: ['clean', 'clean package', '-v'], description: '构建命令', name: 'buildShell')
    }

    stages {
        stage("构建") {
            steps{
                script{
                    mvnHome = tool "M2"
                    sh "${mvnHome}/bin/mvn ${buildShell}"
                }
            }
        }
    }
}
